    "electron": "^13.1.2",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "monaco-editor": "^0.23.0",
    "node-q": "^2.5.0",
    "tabulator-tables": "^4.9.3"
